<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Session ID</th>
        <th scope="col">Type</th>
        <th scope="col">Private</th>
        <th scope="col">Creator</th>
      </tr>
    </thead>
    <tbody>
      <% for (i = 0; i < sessions.length; i++) { %>
        <tr class="clickable-row" data-sid=<%= sessions[i][0] %>>
          <th scope="row"><%= sessions[i][0] %></th>
          <td><%= sessions[i][1].type %></td>
          <td><%= sessions[i][1].private %></td>
          <td><%= sessions[i][1].creator %></td>
        </tr>
      <% } %>
    </tbody>
  </table>